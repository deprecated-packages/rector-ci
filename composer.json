{
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "cache/adapter-bundle": "^1.3",
        "cache/filesystem-adapter": "^1.0",
        "knplabs/github-api": "^2.11",
        "lcobucci/jwt": "^3.3",
        "league/flysystem-bundle": "^1.1",
        "league/oauth2-github": "^2.0",
        "nette/utils": "^3.0",
        "php-http/guzzle6-adapter": "^1.1",
        "ramsey/uuid-doctrine": "^1.5",
        "rector/rector": "dev-master",
        "sensio/framework-extra-bundle": "^5.3",
        "sentry/sentry-symfony": "^3.0",
        "symfony/asset": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/expression-language": "^4.3",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.3",
        "symfony/security-bundle": "^4.3",
        "symfony/serializer-pack": "*",
        "symfony/translation": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/validator": "^4.3",
        "symfony/web-link": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "phpunit/phpunit": "^8.1",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^4.3",
        "symfony/debug-pack": "*",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/profiler-pack": "*",
        "symfony/web-server-bundle": "^4.3",
        "symplify/easy-coding-standard": "^6.0",
        "symplify/phpstan-extensions": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Rector\\RectorCI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\RectorCI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check bin src tests --ansi",
        "fix-cs": [
            "vendor/bin/ecs check bin src tests --fix --ansi",
            "vendor/bin/rector process bin src tests --config rector-coding-style.yaml"
        ],
        "phpstan": "vendor/bin/phpstan analyze bin src tests --error-format symplify --ansi",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": "@auto-scripts",
        "post-update-cmd": "@auto-scripts"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
