{
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "knplabs/github-api": "^2.12",
        "lcobucci/jwt": "^3.3",
        "league/flysystem-bundle": "^1.4",
        "league/oauth2-github": "^2.0",
        "nette/utils": "^3.1",
        "php-http/guzzle6-adapter": "^2.0",
        "ramsey/uuid-doctrine": "^1.5",
        "rector/rector": "dev-master",
        "sensio/framework-extra-bundle": "^5.5",
        "sentry/sentry-symfony": "dev-master",

        "symfony/asset": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.6",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/process": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer-pack": "*",
        "symfony/translation": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/web-link": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^5.0",
        "symfony/debug-pack": "*",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "*",
        "symplify/easy-coding-standard": "^7.2",
        "symplify/phpstan-extensions": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Rector\\RectorCI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\RectorCI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check bin src tests --ansi",
        "fix-cs": [
            "vendor/bin/ecs check bin src tests --fix --ansi",
            "vendor/bin/rector process bin src tests --config rector-coding-style.yaml"
        ],
        "phpstan": "vendor/bin/phpstan analyze bin src tests --error-format symplify --ansi",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": "@auto-scripts",
        "post-update-cmd": "@auto-scripts"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
